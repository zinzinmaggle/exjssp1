<script>  
    var socket = io.connect('http://localhost:3002');
    socket.on('{{{identifiantConnexion}}}', function(data) {
        let test = JSON.parse(data);
        console.log(test);
        for(let i in test['datas']){
          $('#li-'+i).html(test['datas'][i]);
        }
    });
</script>
<div>   
    <div class="uk-card uk-card-small uk-card-default uk-card-body uk-width-auto@m">
        <div class="uk-card-badge uk-label">Hors-ligne</div>
        <h3 class="uk-card-title">{{libelleObjet}}</h3>
        <div >
            <p id="message">{{typeObjetLabel}} <br/> Identifiant de connexion : {{identifiantConnexion}}</p>
            <ul uk-tab='connect: #ul-{{{identifiantConnexion}}}'>
                {{#each datas}}
                    <li class="uk"><a class="uk-tab-root-link" href="">{{libelle}}</a></li>
                {{/each}}
            </ul>
            <ul class="uk-switcher uk-margin" id="ul-{{{identifiantConnexion}}}">
                {{#each datas}}
                    <li id="li-{{{abreviation}}}"></li>
                {{/each}}
            </ul>     
        </div>
    </div> 
</div>